# Cosimulation output content module test

begin: data;
	problem: initial value;
end: data;

begin: initial value;
	initial time: 0.;
	final time: 1.;
	time step: 1e-2;

	tolerance: 1e-6;
	max iterations: 10;

	method: ms, 0.6;
end: initial value;

begin: control data;
	structural nodes: 1;
	joints: 1;
	output elements: 1;
end: control data;

module load: "libmodule-cosim-output.la";
# module load: "libmodule-cosim-output.so";

reference: 99,
	reference, global,
		null,
		# 1., 2., 6.,
	reference, global,
		eye,
		# 1, 1., 0., 0., 2, 0., cos(30.*deg2rad), sin(30.*deg2rad),
	reference, global, null,
	reference, global, null;

begin: nodes;
	structural: 99, dynamic, at reference, 99;
end: nodes;

begin: elements;
	joint: 99, total pin joint,
		99,
			position, reference, node, null,
			position orientation, reference, node, eye,
			rotation orientation, reference, node, eye,
		#
			position, reference, other node, null,
			position orientation, reference, other node, eye,
			rotation orientation, reference, other node, eye,
		position constraint, 1, 1, 1,
			# null,
			1., 0., 0., step5, 0.1, 0., 0.5, 3.,
		orientation constraint, 1, 1, 1,
			# null;
			0., 0., 1., step5, 0.5, 0., 0.9, 2*pi;

	output element: 98, socket stream,
		# send after, convergence,
                stream name, "COSIM",
                create, yes,
                path, "$MBSKOU", # generated by cosim-output.py and passed via environment
                # port, 10011,
		echo, "output.echo",
		# cosimulation, 99, time step, 0.01, rho, 0.;
		# cosimulation, 99;
		# cosimulation, 99, time step, 0.01;
		cosimulation, 99, time step, from solver, rho, 0.6;
end: elements;


# vim:ft=mbd



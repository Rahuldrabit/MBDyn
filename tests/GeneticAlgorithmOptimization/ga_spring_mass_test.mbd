begin: data;
	problem: initial value;
end: data;

begin: initial value;
	initial time: 0.;
	final time: 2.0;
	time step: 1.e-3;
	
	tolerance: 1.e-6;
	max iterations: 10;
	
	# Include all component files
	include: "ga_spring_mass_nodes.mbd";
	include: "ga_spring_mass_elements.mbd";
	include: "ga_spring_mass_drivers.mbd";
	include: "ga_spring_mass_joints.mbd";
	include: "ga_spring_mass_private.mbd";
	include: "ga_spring_mass_super_element.mbd";
	
	# Output configuration
	output: iterations;
	output: residual;
	output: solution;
	
	# GA specific output
	output: genetic algorithm;
end: initial value;

begin: control data;
	structural nodes: 3;
	rigid bodies: 2;
	joints: 2;
	forces: 1;
	genetic algorithm elements: 1;
	max iterations: 100;
	tolerance: 1.e-8;
	print: dof stats;
	print: equation description;
	print: dof description;
	print: element connection;
	print: node connection;
	default output: all;
	output frequency: 10;
end: control data;

GA_SUPER_ELEMENT,
    1001,           ! element ID
    GA_SOLVER(
        name="SpringGA",
        population=30,
        maxgen=100,
        selection="tournament",      # Selection method
        tournament_size=3,
        crossover_rate=0.7,
        mutation_rate=0.15,
        mutation_type="gaussian",
        hill_climbing="yes",         # Enable hill climbing
        hill_climb_rate=0.05,
        convergence_tol=1e-6,
        rng_seed=12345
    ),
    VARIABLES(
        k RANGE [1.0, 200.0]
    ),
    OBJECTIVE(
        type="STATIC_DEFLECTION",
        load=1.0,
        target=0.01
    ),
    CONSTRAINTS(
        k >= 1.0,
        k <= 200.0
    ),
    GA_OUTPUT(
        logfile="spring_ga_test.log",
        write_interval=10,
        write_best_vars="spring_best_test.out"
    ),
END_GA_SUPER_ELEMENT

begin: data;
	problem: initial value;
end: data;

begin: initial value;
	initial time: 0.;
	final time: 1.;
	time step: 1.e-2;
threads: disable;
end: initial value;

begin: control data;
	structural nodes: 2;
	rigid bodies: 2;

	default output: drive callers;
end: control data;

begin: nodes;
	structural: 1, dynamic, null, eye, null, null;
	structural: 2, dynamic, null, eye, 1., 0., 0., null;
end: nodes;

begin: elements;
	drive caller: 1, string, "model::distance(1, 2)", output, yes;
	# drive caller: 1, string, "model::distance(1, 2)", output trace, yes;
	body: 1, 1, 1., null, eye;
	body: 2, 2, 1., null, eye;
end: elements;



begin: data;
    problem: initial value;

end: data;

begin: initial value;
  #  abort after: input;
    time step: (real dt = 1.e-2);
    initial time: 0.;
    final time: 1000*dt;
    
    set: (real Rho = .6);
    method: ms, Rho, Rho;
    
    max iterations: 100;
    tolerance: 1.e-5;
    
    derivatives coefficient: 1.e-6;
    derivatives max iterations: 10;
    derivatives tolerance: 1.e-4;
    
    fictitious steps number: 0;
    fictitious steps ratio: 1.e-4;
    fictitious steps max iterations: 10;
    fictitious steps tolerance: 1.e-4;
threads: disable;
end: initial value;



begin: control data;
 
  hydraulic nodes: 1;       #  numero nodi idraulici
  hydraulic elements: 1;    #  numero elementi idraulici
  forces: 0;                 # una forza generica
  genels: 1;
end: control data;
 
begin: nodes;

   hydraulic: 1, (real p1=1.e4);
#   hydraulic: 2, (real p2=1.e5);
# hydraulic: 1;
#  hydraulic: 2;
# hydraulic: 3;
 #  hydraulic: 4;

end: nodes;



begin: elements;
 
 # density                  800 kg/m^3		kappa		     10 N/m
 # diametro               0.007 m		precarico	    300 N 
 # viscosità    	    0.9 kg/m/s		larghezza          5e-3 m
 # area tubo   	       3.84 e-5 m^2		massa		  1.e-3 kg 
 # lunghezza		      1	m 		area massima      2.e-4 m^2
 # area diaframma        2. e-5 m^2 50% area tubo  
 # Weight peso                  N
 # Spring (costante della molla)    N/m             precarico della molla         N

 
   hydraulic: 11,accumulator,1,
	        0.1,		# stroke
		3.e-2,		# area setto 
		3.84e-5, 	# area tubo  
		4.e-1,		# massa 
		 #gas, 1.e3, 1.e5,1.3, 		# press0 minima gas, press_max gas, kappa della trasformazione
		weight, 1000.,     # Weight peso
	 	spring, 1000.,100.,	# Spring (costante della molla), precarico della molla
		800,            # density
		1.e4,		# c_spost 1.e8,  se press0 >> p1 posso aumetare c1
		10000.,		# c_vel   1000.,
		hydraulic fluid, # incompressible, density, 800.,
			linear compressible, density, 800., 2.e9, 101325.,
				viscosity, 0.0001 # m^2/s == 100 centistokes
		;		# c_acc	   10.;



 # forza generico: generatore di portata
/*   
   force: 111, abstract, 
          1, hydraulic,
          const,-1.;
*/
          
 
# incastro generico: generatore di pressione


begin: data;
    problem: initial value;
end: data;

begin: initial value;
  #  abort after: input;
    time step: (real dt = 1.e4);
    initial time: 0.;
    final time: 100*dt;
    
    set: (real Rho = .6);
    method: ms, Rho, Rho;
    
    max iterations: 1000;
    tolerance: 1.e-5;
    
    derivatives coefficient: 1.e-6;
    derivatives max iterations: 10;
    derivatives tolerance: 1.e-4;
threads: disable;
end: initial value;

begin: control data;
  hydraulic nodes: 3;       #  numero nodi idraulici
  hydraulic elements: 1;    #  numero elementi idraulici
  forces: 1;                 # una forza generica
  genels: 2;
end: control data;
 
begin: nodes;
  hydraulic: 1;
  hydraulic: 2;
  hydraulic: 3;
end: nodes;

begin: elements;
 
 # density                  800 kg/m^3		kappa		     10 N/m
 # diametro               0.007 m		precarico	    300 N 
 # viscosità    	    0.9 kg/m/s		larghezza          5e-3 m
 # area tubo   	       3.84 e-5 m^2		massa		  1.e-3 kg 
 # lunghezza		      1	m 		area massima      2.e-4 m^2
 # area diaframma        2. e-5 m^2 50% area tubo  



 # Parte !  Con un generatore di portata ha bisogno di molte iterazioni (1000 invece di 10)
  hydraulic: 1, flow valve, 1, 2, 3,  # 1:ingresso  2: scarico  3:uscita
            	2.e-5,   	# area diaframma
 	       	1.e-3,  	# massa
		1.e-4,  	# area tubo
		2.e-4,  	# area massima
		10., 		# kappa
		300.,  		# precarico molla
		5.e-3,  	# larghezza
		1.,		# max stroke
		1.e8,		# c_spost 1.e8,
		1000.,		# c_vel   1000.,
		0.,		# c_acc	   10.;
		hydraulic fluid, incompressible, density, 800., viscosity, 0.9;

  
 # forza generico: generatore di portata
   force: 1, abstract, 
          	1, hydraulic,
          	const,-1.;
          	# step,1.e-3,1.e-3,1.e-3;
          	# array, 2,
	# random, .01,0.,dt,1000., steps, 10;
	# sine, 0.,pi,1.e-4,0,0.;  
          	# linear, -1.-2,-1.e-5;
          	#  step,5.,-1.e-6,-1.e-3;
 
# incastro generico: generatore di pressione
   genel: 1, clamp, 
             2, hydraulic, 
	     const, 1.e3;
	    # const, p2;
            # step,2.,1.e8,1.e5;
            #linear, 1,-1.e6;
	    # sine, 0.1,pi,1.e6,0,500000.;

# incastro generico: generatore di pressione
   genel: 2, clamp, 
             3, hydraulic, 
	     const, 1e4;
            # step,2.,1.e5,1.e8;
            #linear, 1.e7,1.e6;
	    # sine, 0.1,pi,1.e6,0,500000.;
end: elements;


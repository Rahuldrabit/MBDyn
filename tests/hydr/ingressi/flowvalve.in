begin: data;
    problem: initial value;

end: data;

begin: initial value;
  #  abort after: input;
    time step: (real dt = 1.e4);
    initial time: 0.;
    final time: 100*dt;
    
    set: (real Rho = .6);
    method: ms, Rho, Rho;
    
    max iterations: 1000;
    tolerance: 1.e-5;
    
    derivatives coefficient: 1.e-6;
    derivatives max iterations: 10;
    derivatives tolerance: 1.e-4;
    
    fictitious steps number: 2;
    fictitious steps ratio: 1.e-4;
    fictitious steps max iterations: 10;
    fictitious steps tolerance: 1.e-4;

linear solver: umfpack, workspace size, 10000;
threads: disable;
end: initial value;



begin: control data;
#  output file name: "output";

  
  
  hydraulic nodes: 3;       #  numero nodi idraulici
 
  hydraulic elements: 1;    #  numero elementi idraulici
                            
  

  forces: 0;                 # una forza generica

  
  genels: 3;
                   
 
 end: control data;
 
begin: nodes;

#   hydraulic: 1, (real p1=12.e5);
#   hydraulic: 2, (real p2=2.e5);
  hydraulic: 1;
 hydraulic: 2;
  hydraulic: 3;
#   hydraulic: 4;

end: nodes;



begin: elements;
 

		800,		# densità
            	2.e-5,   	# area diaframma
 	        1.e-3,  	# massa
		2.e-4,  	# area massima
		10., 		# kappa
		300,  		# precarico molla
		5.e-3,  	# larghezza
		1.e8,		# c_spost 1.e8,
		0,		# c_vel   1000.,

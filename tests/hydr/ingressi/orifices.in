begin: data;
    problem: initial value;

end: data;

begin: initial value;
  #  abort after: input;
    time step: (real dt = 2.e-4);
    initial time: 0.;
    final time: 150*dt;
    
    set: (real Rho = .6);
    method: ms, Rho, Rho;
    
    max iterations: 10;
    tolerance: 1.e-5;
    
    derivatives coefficient: 1.e-6;
    derivatives max iterations: 10;
    derivatives tolerance: 1.e-4;
    
    fictitious steps number: 2;
    fictitious steps ratio: 1.e-2;
    fictitious steps max iterations: 10;
    fictitious steps tolerance: 1.e-4;

threads: disable;
end: initial value;



begin: control data;
#  output file name: "output";

  
  
  hydraulic nodes: 2;       #  numero nodi idraulici
 
  hydraulic elements: 1;    #  numero elementi idraulici
                            
  

  forces: 1;                 # una forza generica

  
  genels: 1;
                   
 
 end: control data;
 
begin: nodes;


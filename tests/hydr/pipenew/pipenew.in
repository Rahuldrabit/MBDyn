begin: data;
    problem: initial value;

end: data;

begin: initial value;
  #  abort after: input;
    time step: (real dt = 1.e10);
    initial time: 0.;
    final time: 30*dt;
    
    set: (real Rho = .6);
    method: ms, Rho, Rho;
    
    max iterations: 10;          
    tolerance: 1.e-6;
    
    derivatives coefficient: 1.e-4;
    derivatives max iterations: 10;  
    derivatives tolerance: 1.e-6;
    
    fictitious steps number: 0;
    fictitious steps ratio: 1.e-4;
    fictitious steps max iterations: 10;
    fictitious steps tolerance: 1.e-6;

linear solver: umfpack, workspace size, 10000;
threads: disable;
end: initial value;



begin: control data;
#  output file name: "output";

  
  
  hydraulic nodes: 2;       #  numero nodi idraulici
 
  hydraulic elements: 1;    #  numero elementi idraulici
                            
  

  forces: 0;               # una forza generica

  
  genels: 2;
                   
 
 end: control data;
 
begin: nodes;

  hydraulic: 1,  (real p1=5e+8);
#   hydraulic: 1, (real p1=5.6966803e+08);
   hydraulic: 2,  (real p2=1.e3);
#  hydraulic: 2;
#  hydraulic: 3;
#  hydraulic: 4;

end: nodes;



begin: elements;
 
 # density                  800 kg/m^3		kappa		     10 N/m
 # diametro               0.007 m		precarico	    300 N 
 # viscosità    	    0.9 kg/m/s		larghezza          5e-3 m
 # area tubo   	       3.84 e-5 m^2		massa		  1.e-3 kg 
 # lunghezza		      1	m 		area massima      2.e-4 m^2
 # area diaframma        2. e-5 m^2 50% area tubo  


 


  
  hydraulic: 1,pipe new,2,1,
		800.,			# densità
		.007,			# diametro
		.9,			# viscosità
		3.84e-5,		# area
		1.,			# lunghezza
		turbulent;		# flag turbulent
                # initial value, 16; 	# portata iniziale      	
                


  
 # forza generico: generatore di portata
/*
   force: 1, abstract, 
          1, hydraulic,
           const,-19; 
	  #  const,-18.00005;
          # linear, -9.,-0.5/dt; # con dt =1 transizione lam > turb completa
          #   linear, -20.5,0.5/dt; # con dt =1 transizione turb > lam completa
         # step,50.*dt,-19,-15;
*/
 
# incastro generico: generatore di pressione


   genel: 1, clamp, 
             1, hydraulic, 
            const, p1;       # const, p1;
            # step,50*dt,2.e3,2.e7;  # fine inizio
            # linear, 1.8e7,1.5e8; # per il tubo
            #linear, 1.e5,40000./dt;  # per la valvola di pressione
	    #linear, 1.e5,10;
            #sine, 0.1,pi,16*p1,0,0.;
            #double step, 25*dt,75*dt,1e6*p1,p1;


# incastro generico: generatore di pressione

   genel: 2, clamp, 
             2, hydraulic, 
	     const, 1.e3;
            # step,2.,1.e8,1.e5;
            #linear, 1,-1.e6;
	    # sine, 0.1,pi,1.e6,0,500000.;




end: elements;


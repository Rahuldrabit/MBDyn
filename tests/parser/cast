remark: "bool(10.5)", bool(10.5);
remark: "integer(10.5)", integer(10.5);
remark: "real(10.5)", real(10.5);
remark: "string(10.5)", string(10.5);

remark: "\"10::20\"", "\"" + 10 + "::" + 20 + "\"";
remark: "10::20", string(10) + "::" + 20;

remark: "string(\"ciao\")", string("ciao");

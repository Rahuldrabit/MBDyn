# Benchmarks from K.Y. Sze, X.H. Liu, S.H. Lo,
# "Popular benchmark problems for geometric nonlinear analysis of shells",
# Finite Elements in Analysis and Design 40 (2004) 1551Â­1569
# doi:10.1016/j.finel.2003.11.001
# 
# Author: Pierangelo Masarati <masarati@aero.polimi.it> (except when noted)
# 
# 3.1 Cantilever subjected to end shear force
#
# 8x1 mesh
#
# EJ = 100
# L = 10
# A = 0.1
# w_0 = 10 Hz
# rho = (1.87510406871197 / L)^4 * EJ / (w_0^2 * A)

begin: data;
	problem: initial value;
end: data;

begin: initial value;
	initial time: 0.;
	final time: 1.;
	time step: 1e-3;

	tolerance: 1e-6;
	max iterations: 50;

	method: ms, .6;

	eigenanalysis:
		# 0.0005,
		list, 6, 0., 0.1995, 0.3995, 0.5995, 0.7995, 0.9995,
		lower frequency limit, 0.1,
		upper frequency limit, 200.,
		output geometry,
		output eigenvectors,
		use lapack;

	output: iterations;
threads: disable;
end: initial value;

begin: control data;
	structural nodes:
		+ (1 + 8)*2
	;
	rigid bodies:
		+ 8*2
	;
	joints:
		+ 2
	;
	plates:
		+ 8
	;
	forces:
		+ 2
	;
end: control data;

set: const real E = 1.2e6;
set: const real nu = 0.;
set: const real L = 10.;
set: const real b = 1.;
set: const real h = 0.1;
set: const real A = b * h;
set: const real EJ = 1.*L^2;
set: const real omega_0 = 10.*2*pi;
set: const real rho = (1.87510406871197 / L)^4 * EJ / (omega_0^2 * A);
set: const real P0 = 1.;
set: const real PMAX = 4*P0;

remark: "rho", rho;

reference: 0,
	reference, global, null,
	reference, global, eye,
	reference, global, null,
	reference, global, null;

reference: 100,
	reference, 0, 0., -b/2, 0.,
	reference, 0, eye,
	reference, 0, null,
	reference, 0, null;

reference: 200,
	reference, 0, 0., b/2, 0.,
	reference, 0, eye,
	reference, 0, null,
	reference, 0, null;

begin: nodes;
	structural: 100, static,
		reference, 100, 0., 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 101, dynamic,
		reference, 100, 1./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 102, dynamic,
		reference, 100, 2./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 103, dynamic,
		reference, 100, 3./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 104, dynamic,
		reference, 100, 4./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 105, dynamic,
		reference, 100, 5./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 106, dynamic,
		reference, 100, 6./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 107, dynamic,
		reference, 100, 7./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;
	structural: 199, dynamic,
		reference, 100, 8./8.*L, 0., 0.,
		reference, 100, eye,
		reference, 100, null,
		reference, 100, null;

	structural: 200, static,
		reference, 200, 0., 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 201, dynamic,
		reference, 200, 1./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 202, dynamic,
		reference, 200, 2./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 203, dynamic,
		reference, 200, 3./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 204, dynamic,
		reference, 200, 4./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 205, dynamic,
		reference, 200, 5./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 206, dynamic,
		reference, 200, 6./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 207, dynamic,
		reference, 200, 7./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
	structural: 299, dynamic,
		reference, 200, 8./8.*L, 0., 0.,
		reference, 200, eye,
		reference, 200, null,
		reference, 200, null;
end: nodes;

set: real M = (L/8)*h*(b/2)*rho;
set: real J_X = M*(b/2)^2/12;
set: real J_Y = M*(L/8)^2/12;
set: real J_Z = J_X + J_Y;
set: real y_offset = b/4;
set: real x_offset = (L/8)/4;

drive caller: 99,
	cosine, 0., pi/1., (PMAX/2)/2, half, 0.;
	# sine, 0., pi/.1, .01, one, 0.;
begin: elements;
	joint: 100, clamp, 100, node, node;
	joint: 200, clamp, 200, node, node;

	body: 101, 101,
		M,
		reference, node, 0., y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 102, 102,
		M,
		reference, node, 0., y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 103, 103,
		M,
		reference, node, 0., y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 104, 104,
		M,
		reference, node, 0., y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 105, 105,
		M,
		reference, node, 0., y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 106, 106,
		M,
		reference, node, 0., y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 107, 107,
		M,
		reference, node, 0., y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 108, 199,
		M/2,
		reference, node, -x_offset, y_offset, 0.,
		diag, J_X/2, J_Y/8, J_X/2 + J_Y/8;

	body: 201, 201,
		M,
		reference, node, 0., -y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 202, 202,
		M,
		reference, node, 0., -y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 203, 203,
		M,
		reference, node, 0., -y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 204, 204,
		M,
		reference, node, 0., -y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 205, 205,
		M,
		reference, node, 0., -y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 206, 206,
		M,
		reference, node, 0., -y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 207, 207,
		M,
		reference, node, 0., -y_offset, 0.,
		diag, J_X, J_Y, J_Z;
	body: 208, 299,
		M/2,
		reference, node, -x_offset, -y_offset, 0.,
		diag, J_X/2, J_Y/8, J_X/2 + J_Y/8;

	shell4easans: 1,
		201, 200, 100, 101,
		isotropic, E, E, nu, nu, thickness, h;
	shell4easans: 2,
		202, 201, 101, 102,
		isotropic, E, E, nu, nu, thickness, h;
	shell4easans: 3,
		203, 202, 102, 103,
		isotropic, E, E, nu, nu, thickness, h;
	shell4easans: 4,
		204, 203, 103, 104,
		isotropic, E, E, nu, nu, thickness, h;
	shell4easans: 5,
		205, 204, 104, 105,
		isotropic, E, E, nu, nu, thickness, h;
	shell4easans: 6,
		206, 205, 105, 106,
		isotropic, E, E, nu, nu, thickness, h;
	shell4easans: 7,
		207, 206, 106, 107,
		isotropic, E, E, nu, nu, thickness, h;
	shell4easans: 8,
		299, 207, 107, 199,
		isotropic, E, E, nu, nu, thickness, h;

	force: 100, absolute,
		199,
		position, null,
		0., 0., 1., reference, 99;
	force: 200, absolute,
		299,
		position, null,
		0., 0., 1., reference, 99;
end: elements;

# vim:ft=mbd

